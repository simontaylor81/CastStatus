{
  "version": "2.0.0",
  "isShellCommand": true,
  "suppressTaskName": true,
  "tasks": [
    {
      "taskName": "watch",
      "command": "${workspaceRoot}/npm-server.bat",
      "args": [ "run", "watch" ],
      "isBackground": true,
      "problemMatcher": "$tsc-watch"
    },
    {
      "taskName": "Start dev server",
      "command": "${workspaceRoot}/npm-client.bat",
      "args": [ "run", "dev" ],
      "isBackground": true
    }
  ]
}
